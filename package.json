{
  "name": "auth-server",
  "version": "0.0.5",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "BSD-3-Clause-Clear",
  "description": "auth server for fabric-es project",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/rtang03/auth-server.git"
  },
  "keywords": [
    "oauth",
    "passport"
  ],
  "author": "Ross Tang",
  "homepage": "https://github.com/rtang03/auth-server",
  "bugs": {
    "url": "https://github.com/rtang03/auth-server/issues"
  },
  "scripts": {
