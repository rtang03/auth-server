    "http-status": "^1.4.1",
    "ioredis": "^4.19.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "nodemailer": "^6.4.17",
    "oauth2orize": "^1.11.0",
    "passport": "^0.4.1",
